style "murrine-button" = "murrine-wider"
{
	bg[NORMAL]      = shade (1.04, @bg_color)
	bg[PRELIGHT]    = shade (1.06, @bg_color)
	bg[ACTIVE]      = shade (1.02, @bg_color)
	bg[INSENSITIVE] = shade (1.0, @bg_color)

	engine "murrine" {
		contrast            = 0.45
		glow_shade = 1.20
		lightborder_shade = 1.06
	}
}

